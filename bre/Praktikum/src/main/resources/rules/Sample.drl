 rule "Ã–rtliche Filterung"
 	salience 10
 	when
 		m : AixmMessage ( location != FlightPath.getDepatureAerodrome() || location != FlightPath.getDestinationAerodrome())
 	
 	
 	then
 		System.out.println("not relevant at " + m.getScenario());
 		m.setRelevant(false);
 		retract(m);
 
 end
 
 rule "Zeitliche Filterung"
 	salience 11
 	when
 		m: AixmMessage ( begintime < TimePeriod.getBeginPosition() && endtime < TimePeriod.getBeginPosition() || begintime > TimePeriod.getEndPosition() && endtime > TimePeriod.getEndPosition())  
 	then
 		System.out.println("not relevant at " + m.getScenario());
 		m.setRelevant(false);
 		retract(m);
 end
